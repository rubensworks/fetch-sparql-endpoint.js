{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": [
      "tsconfig.eslint.json"
    ]
  },
  "extends": [
    "@rubensworks"
  ],
  "ignorePatterns": [
    "*.d.ts"
  ],
  "rules": {
    "no-implicit-coercion": "off",
    "import/group-exports": "off",
    "import/no-nodejs-modules": "off",
    "import/no-unassigned-import": "off"
  }
}
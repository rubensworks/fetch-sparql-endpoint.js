{
  "name": "fetch-sparql-endpoint",
  "version": "1.1.0",
  "description": "A simple, lightweight module to send queries to SPARQL endpoints and retrieve their results in a streaming fashion.",
  "keywords": [
    "sparql",
    "query",
    "semantic web",
    "rdf",
    "rdfjs",
    "streaming"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/fetch-sparql-endpoint.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/fetch-sparql-endpoint.js/issues"
  },
  "homepage": "https://github.com/rubensworks/fetch-sparql-endpoint.js#readme",
  "license": "MIT",
  "bin": {
    "fetch-sparql-endpoint": "fetch-sparql-endpoint.js"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "dependencies": {
    "is-stream": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "minimist": "^1.2.0",
    "n3": "^1.0.0-beta.1",
    "node-web-streams": "^0.2.2",
    "rdf-string": "^1.2.0",
    "sparqljs": "^2.0.3",
    "sparqljson-parse": "^1.2.0",
    "stream-to-string": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/minimist": "^1.2.0",
    "@types/rdf-js": "^1.0.1",
    "@types/sparqljs": "^1.5.0",
    "arrayify-stream": "^1.0.0",
    "coveralls": "^3.0.0",
    "jest": "^23.5.0",
    "jest-rdf": "^1.0.0",
    "pre-commit": "^1.2.2",
    "@rdfjs/data-model": "^1.1.0",
    "streamify-string": "^1.0.1",
    "ts-jest": "^23.1.4",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "jest-rdf",
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  }
}

{
  "name": "fetch-sparql-endpoint",
  "version": "4.1.1",
  "description": "A simple, lightweight module to send queries to SPARQL endpoints and retrieve their results in a streaming fashion.",
  "packageManager": "yarn@4.1.1",
  "keywords": [
    "sparql",
    "query",
    "semantic web",
    "rdf",
    "rdfjs",
    "streaming"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/fetch-sparql-endpoint.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/fetch-sparql-endpoint.js/issues"
  },
  "homepage": "https://github.com/rubensworks/fetch-sparql-endpoint.js#readme",
  "license": "MIT",
  "bin": "bin/fetch-sparql-endpoint.js",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "bin/**/*.js.map",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "index.d.ts",
    "index.js.map",
    "index.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run build && yarn run lint && yarn run test"
    }
  },
  "dependencies": {
    "@rdfjs/types": "*",
    "@smessie/readable-web-to-node-stream": "^3.0.3",
    "@types/readable-stream": "^4.0.0",
    "@types/sparqljs": "^3.0.0",
    "cross-fetch": "^4.0.0",
    "is-stream": "^2.0.0",
    "n3": "^1.0.0",
    "rdf-string": "^1.0.0",
    "sparqljs": "^3.0.0",
    "sparqljson-parse": "^2.0.0",
    "sparqlxml-parse": "^2.0.0",
    "stream-to-string": "^1.0.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@rubensworks/eslint-config": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/n3": "^1.0.0",
    "arrayify-stream": "^2.0.0",
    "coveralls": "^3.0.0",
    "eslint": "8.48.0",
    "husky": "^4.0.0",
    "jest": "^29.0.0",
    "jest-rdf": "^1.0.0",
    "manual-git-changelog": "^1.0.0",
    "rdf-data-factory": "^1.0.0",
    "readable-stream-node-to-web": "^1.0.0",
    "streamify-string": "^1.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "jest-rdf"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc",
    "validate": "yarn list",
    "prepare": "yarn run build",
    "version": "manual-git-changelog onversion"
  },
  "sideEffects": false
}
